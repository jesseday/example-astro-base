---
import Section from "./Section.astro";

const Wrapper = Astro.props.contain ? Section : Fragment;
const forward = Astro.props.forward || {};
---

<Wrapper {...forward}>
  <div data-component-id="Duplex">Duplex component</div>
</Wrapper>

<style>
  /* Default css. Remove and add your own styles */
  [data-component-id="Duplex"] {
    display: block;
    padding: 1rem;
    border: 1px dashed #ccc;
    width: 100%;
    height: 300px;
  }
</style>
